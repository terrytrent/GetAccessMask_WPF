<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		Title="Get Access Mask - Results" Height="135" Width="325" ResizeMode="NoResize" ShowInTaskbar="False" Topmost="false" WindowStartupLocation="CenterOwner">
    <Grid Background="#FFF0F0F0">
        <Border Margin="10" Background="White" BorderBrush="#FFB4B4B4" BorderThickness="1">
            <Grid Margin="9">
                <Label Name="lbl_AccessMask" Content="AccessMask" VerticalAlignment="Top" Padding="0" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" FontSize="21.333"/>
                <Button Name="btn_CopyToClipboard" Content="Copy To Clipboard" HorizontalAlignment="Left" Margin="5,0,0,0" VerticalAlignment="Bottom" Background="#FFDDDDDD" BorderBrush="#FF707070" Width="125"/>
                <Button Name="btn_Close" Content="Close" HorizontalAlignment="Right" Margin="0,0,5,0" VerticalAlignment="Bottom" Background="#FFDDDDDD" BorderBrush="#FF707070" Width="125"/>

            </Grid>
        </Border>

    </Grid>
</Window>
